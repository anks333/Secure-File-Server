<!DOCTYPE html>
<html>
<head>

    <title>Secure File Server</title>
    <base href="/" >
    <link rel="stylesheet" href="font/font-awesome-4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="libs/angular-material/angular-material.min.css">

    <script type="text/javascript" src="libs/angular/angular.min.js"></script>
    <script type="text/javascript" src="libs/angular-route/angular-route.min.js"></script>
    <script type="text/javascript" src="libs/angular-local-storage/dist/angular-local-storage.min.js"></script>

    <script type="text/javascript" src="libs/angular-animate/angular-animate.min.js"></script>
    <script type="text/javascript" src="libs/angular-aria/angular-aria.min.js"></script>
    <script type="text/javascript" src="libs/angular-messages/angular-messages.min.js"></script>
    <script type="text/javascript" src="libs/angular-material/angular-material.min.js"></script>


    <script type="text/javascript" src="app.module.js"></script>
    <script type="text/javascript" src="js-bundle.min.js"></script>
    <script type="text/javascript" src="app.js"></script>
</head>
<body ng-app="secureFileServer">

    <div layout="column" layout-fill ng-controller="TabController" >
        <md-toolbar >
            <div class="md-toolbar-tools">
                <h2><span style="margin:10px;" ng-click="toggleLeft('left')"><i class="fa fa-bars"></i></span><span> Secure File Server</span></h2>
            </div>
            
        </md-toolbar>

        <div layout="row">

            <md-sidenav md-component-id="left" class="md-sidenav-left md-whiteframe-z2" md-is-locked-open="$mdMedia('gt-md')" style="height: 100vh" >
                <md-toolbar hide-gt-md>
                    <div class="md-toolbar-tools">
                        <span flex="80"></span>
                        <span flex="20" ng-click="toggleLeft('left')"><i class="fa fa-bars"></i></span>
                    </div>
                </md-toolbar>
                <md-content layout-padding layout-wrap>
                    
                    <p ng-click="logout()" ng-if="isLoggedIn">Logout</p> 
                            
                </md-content>
                <span style="color:white">{{mediaVal('gt-md')}}</span>

            </md-sidenav>
            <md-content flex>
                <div ng-view></div>
            </md-content>
        </div>

    </div>

</body>
</html>
